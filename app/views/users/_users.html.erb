<% if users.any? %>
    <ul class="list-unstyled">
        <% users.each do |user| %>
            <div class="media-body">
                <%= user.name %>
            </div>
            <div>
                <p><%= link_to 'View profile', user_path(user) %></p>
            </div>
        <% end %>
    </ul>
<% end %>